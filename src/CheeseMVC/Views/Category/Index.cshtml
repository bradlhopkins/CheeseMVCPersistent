@model List<CheeseMVC.Models.CheeseCategory>

<h1>@ViewBag.Title</h1>


    @foreach (var category in Model)
    {
            <ul>@category.Name</ul>
            
            
        
    }



@if (Model.Count == 0)
{
    <p>No categories yet!</p>
}

<p><a asp-controller="CheeseCategory" asp-action="Add">Add Category</a></p>

